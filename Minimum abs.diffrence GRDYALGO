import java.util.*;
public class Main
{
	public static void main(String[] args) 
	{
	    int a[]={1,3,5};
	    int b[]={3,4,5};
	    Arrays.sort(a);
	    Arrays.sort(b);
	    int mindiff = 0;
	    for(int i=0;i<a.length;i++)
	    {
	        mindiff+=Math.abs(a[i]-b[i]);
	    }
	    System.out.print("min diffrence is "+mindiff);
	}
}
